    "#### TODO: Get value between \"\" in name and put it in category column"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 185,
   "metadata": {},
   "outputs": [],
   "source": [
    "result = pd.Series()\n",
    "for index,name in df['name'].iteritems():\n",
    "    string = re.search('\"(.*)\"', name).group(0)\n",
    "    result.at[index] = string.strip('\\\"')"
   ]
  },
